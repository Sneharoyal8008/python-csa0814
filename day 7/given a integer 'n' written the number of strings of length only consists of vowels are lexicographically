from itertools import product

def generate_vowel_strings(n):
    vowels = 'aeiou'
    # Generate all combinations of length n with the given vowels
    result = [''.join(p) for p in product(vowels, repeat=n)]
    return result

# Example usage
n = 3  # Example length of strings
strings = generate_vowel_strings(n)
for s in strings:
    print(s)

